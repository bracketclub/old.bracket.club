    "build-analyze": "WEBPACK_ANALYZE=true npm run build",
    "build-prod": "CONFIG_ENV=live npm run build",
    "build-static": "CONFIG_ENV=static npm run build",
    "codeship-deploy": "npm run firebase -- deploy",
    "codeship-deploy-beta": "surge --project build/ --domain beta.bracket.club",
    "codeship-setup": "npm install && npm run build-prod",
    "codeship-test": "npm test && npm run lint",
    "firebase": "firebase",
    "lint": "eslint .",
    "pr": "open 'https://github.com/bracketclub/bracket.club/compare/master...development?expand=1'",
    "prebuild": "rm -rf build && mkdir build",
    "start": "hjs-dev-server",
    "test": "jest --config jest/config.json --rootDir . --testPathPattern __tests__/[a-z].*",
    "test:watch": "npm test -- --watch",
    "validate": "npm ls >/dev/null"
